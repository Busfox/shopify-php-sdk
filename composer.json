{
    "name": "robby-bugatti/shopify-php-sdk",
    "description": "PHP SDK for Shopify API",
    "keywords": [
        "shopify",
        "ecommerce",
        "api",
        "php"
    ],
    "homepage": "http://github.com/RobbyBugatti/shopify-php-sdk",
    "license": "MIT",
    "authors": [],
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2",
        "netresearch/jsonmapper": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Shopify\\" : "lib/",
            "Shopify\\Test\\" : "test/"
        },
        "files" : [
            "lib/includes.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    }
}
